<Window x:Class="CWC_2_RosterEditor.ChooseRoster"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CWC_2_RosterEditor"
        mc:Ignorable="d"
        Title="Choose your roster" Height="150" Width="220">
    <Grid>
        <ComboBox x:Name="RostersLits" HorizontalAlignment="Center" Margin="0,16,0,0" VerticalAlignment="Top" Width="200" Height="42"/>
        <Button Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Click="ApplyRoster" Width="66" Margin="28,72,0,0" Height="39"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="115,72,0,0" VerticalAlignment="Top" Click="Cancel" Height="39" Width="69"/>

    </Grid>
</Window>
